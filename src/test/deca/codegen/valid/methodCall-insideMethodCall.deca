class A {
    int x;
    int y;

    int random() {
        return 0;
    }

    boolean isEquals(int x, boolean y) {
        if (y) {
            return x == x;
        }
        return false;
    }

    boolean isEqualsIntern() {
        return isEquals(random(), true);
    }
}

{
    A a = new A();
    if (a.isEqualsIntern()) {
        println("ok");
    }
}